    <text>
        <body>
            <div type="index_persons">
                <listPerson>
                    <person xml:id="widmann">
                        <persName>
                            <forename>Michael</forename>
                            <surname>Widmann</surname>
                            <roleName>Vikar von Fieberbrunn</roleName>
                        </persName>
                        <note>
                            Michael Widmann war von 1787 bis 1799 Vikar in Fieberbrunn. Vgl. Dorfbuch Fieberbrunn, S. 80.
                        </note>
                    </person>
                    <person xml:id="schmutzer">
                        <persName>
                            <forename>Benno</forename>
                            <surname>Schmutzer</surname>
                            <roleName>Vikar von Fieberbrunn</roleName>
                        </persName>
                        <note>
                            Benno Schmutzer war von 1771 bis 1787 Vikar in Fieberbrunn. Vgl. Dorfbuch Fieberbrunn, S. 80.
                        </note>
                    </person>
<!--  @key im persName Element entpspricht der GND-Nummer                    -->
                    <person xml:id="millinger_stefan_vater">
                        <persName>
                            <forename>Stefan</forename>
                            <surname>Millinger</surname>
                            <roleName>Vater von Leonhard Millinger</roleName>
                        </persName>
                    </person>                   
                    <person xml:id="millinger_stefan_sohn">
                        <persName>
                            <forename>Stefan</forename>
                            <surname>Millinger</surname>
                            <roleName>Sohn von Leonhard Millinger</roleName>
                        </persName>
                    </person>
                    <person xml:id="millinger_katharina_frau">
                        <persName>
                            <forename>Katharina</forename>
                            <surname>Millinger, geb. Prantner</surname>
                            <roleName>Frau von Leonhard Millinger</roleName>
                        </persName>
                    </person>
                    <person xml:id="milinger_simon_onkel">
                        <persName>
                            <forename>Simon</forename>
                            <surname>Millinger</surname>
                            <roleName>Onkel von Leonhard Millinger</roleName>
                        </persName>
                    </person>
                    <person xml:id="millinger_leonhard">
                        <persName key="millinger_leonhard">
                            <forename>Leonhard</forename>
                            <surname>Millinger</surname>
                            <roleName>Verfasser der Weltbeschreibung</roleName>
                        </persName>
                        <birth when="1753-04-03">
                            <placeName>
                                <country>Österreich</country>
                                <settlement>Waidring</settlement>
                            </placeName>
                        </birth>
                        <death when="1834-09-20">
                            <placeName>
                                <country>Österreich</country>
                                <settlement>Waidring</settlement>
                            </placeName>
                        </death>
                    </person>
                    <person xml:id="Unbekannt_Person">
                        <persName>
                            <forename>unbekannte Person</forename>
                        </persName>
                        <note>
                            Diese Person konnte noch nicht näher identifiziert werden
                        </note>
                    </person>
                </listPerson>
            </div>
        </body>
    </text>
</TEI>